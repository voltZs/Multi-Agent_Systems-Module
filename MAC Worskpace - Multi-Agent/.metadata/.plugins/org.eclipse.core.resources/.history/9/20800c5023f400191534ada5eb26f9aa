package mas.coursework_ontology.elements;

import jade.content.Predicate;
import jade.content.onto.annotations.Slot;

public class CalendarNotification implements Predicate {
	private boolean newDay;
	private boolean dayDone;
	
	public boolean isNewDay() {
		return newDay;
	}
	public void setNewDay(boolean newDay) {
		this.newDay = newDay;
	}
	public boolean isDayDone() {
		return dayDone;
	}
	public void setDayDone(boolean dayDone) {
		this.dayDone = dayDone;
	}
}
