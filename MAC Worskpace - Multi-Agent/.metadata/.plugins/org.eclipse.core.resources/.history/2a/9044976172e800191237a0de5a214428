
public class BidTuple {
	private String left;
	private int right;
	
	public BidTuple(String left, int right){
		this.left = left;
		this.right = right;
	}
	
	public void setKey(String left){
		this.left = left;
	}
	
	public void setValue(int right){
		this.right = right;
	}
	
	public String getKey(){
		return this.left;
	}
	
	public int getValue(){
		return this.right;
	}
	
	public int get(String match){
		if (match.equals(left)){
			return right;
		} else {
			return 0;
		}
	}
s
}
